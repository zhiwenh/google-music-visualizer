{
  "manifest_version": 2,
  "name": "Google Music Equalizer",
  "description": "Audio equalizer for Google Music",
  "version": "1.0",

  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
      "scripts": ["scripts/jquery-3.0.0.min.js"],
      "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Google Music Equalizer"
  },

  "content_scripts": [
    {
      "matches": ["https://play.google.com/music/*"],
      "js": ["scripts/jquery-3.0.0.min.js",
        "scripts/d3.min.js",
        "scripts/d3-music-equalizer.js",
        "scripts/content.js"
      ]
    }
  ]
}
